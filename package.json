{
  "name": "@iamkenos/fragile",
  "version": "0.1.0",
  "license": "MIT",
  "author": "iamkenos",
  "description": "",
  "main": "./build/index.js",
  "bin": {
    "fragile": "bin/fragile.js"
  },
  "homepage": "https://github.com/iamkenos/fragile#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamkenos/fragile.git"
  },
  "bugs": {
    "url": "https://github.com/iamkenos/fragile/issues"
  },
  "scripts": {
    "start": "npm run build && node bin/ellie",
    "prebuild": "rimraf build && npm run lint -- --fix",
    "build": "tsc",
    "postbuild": "run-s postbuild:**",
    "postbuild:cp:types": "copyfiles -u 1 \"./src/**/types/**\" build",
    "postbuild:cp:resources": "copyfiles -u 1 \"./src/**/resources/**\" build",
    "postbuild:cp:prettierrc": "copyfiles \"./.prettierrc\" build/cli/resources",
    "demo:serve": "http-server ./demo/app -s",
    "demo:test": "./bin/ellie.js demo/test/demo.conf.ts demo/test/overrides.conf.js",
    "demo:test:bs": "./bin/ellie.js demo/test/demo.browserstack.conf.ts",
    "pack": "npm run build && rimraf *ellie-*.tgz && npm pack",
    "lint": "eslint ./",
    "test": "run-s test:**",
    "test:demo": "start-test demo:serve 8080 demo:test",
    "test:demo:bs": "start-test demo:serve 8080 demo:test:bs",
    "test:samples": "./bin/ellie.js src/cli/resources/samples/ellie.conf.ts",
    "test:unit": "jest --config .jestrc.json --coverage",
    "release": "npm run build && node ./.scripts/s_release.js"
  },
  "dependencies": {
    "@types/express": "4.17.11",
    "body-parser": "^1.19.0",
    "body-parser-xml": "^2.0.1",
    "chalk": "4.1.0",
    "cors": "^2.8.5",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "fs-extra": "9.0.1",
    "inquirer": "7.3.3",
    "loglevel": "1.7.0",
    "loglevel-plugin-prefix": "0.8.4",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.5",
    "yargs": "16.1.0"
  },
  "devDependencies": {
    "@types/body-parser-xml": "^1.1.0",
    "@types/cors": "^2.8.9",
    "@types/eslint": "7.2.6",
    "@types/fs-extra": "9.0.6",
    "@types/glob": "7.1.3",
    "@types/inquirer": "7.3.1",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/prettier": "2.1.6",
    "@types/request": "2.48.5",
    "@types/rimraf": "3.0.0",
    "@types/xml2js": "^0.4.8",
    "@types/yargs": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.6.1",
    "@typescript-eslint/parser": "4.6.1",
    "copyfiles": "2.4.1",
    "dotenv": "^8.2.0",
    "eslint": "7.13.0",
    "eslint-config-standard": "16.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "ts-jest": "26.4.4"
  }
}
