{
  "name": "@iamkenos/fragile",
  "version": "0.1.0",
  "license": "MIT",
  "author": "iamkenos",
  "description": "",
  "main": "./build/index.js",
  "bin": {
    "fragile": "bin/fragile.js"
  },
  "homepage": "https://github.com/iamkenos/fragile#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/iamkenos/fragile.git"
  },
  "bugs": {
    "url": "https://github.com/iamkenos/fragile/issues"
  },
  "scripts": {
    "start": "npm run build && node bin/fragile",
    "prebuild": "rimraf build && npm run lint -- --fix",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "run-s postbuild:**",
    "postbuild:cp:types": "mkdir -p build/types && shx cp -R ./src/types build/types",
    "postbuild:cp:resources": "shx cp -R ./src/cli/resources build/cli",
    "postbuild:cp:prettierrc": "shx cp ./.prettierrc build/cli/resources",
    "demo:serve": "./bin/fragile.js demo/demo.conf.ts",
    "pack": "npm run build && rimraf *fragile-*.tgz && npm pack",
    "lint": "eslint ./",
    "test": "run-s test:**",
    "test:demo": "start-test demo:serve 8080 demo:test",
    "test:unit": "jest --config .jestrc.json --coverage",
    "release": "npm run build && node ./.scripts/s_release.js"
  },
  "dependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/register": "7.12.13",
    "@types/express": "4.17.11",
    "@types/express-rate-limit": "5.1.1",
    "body-parser": "1.19.0",
    "body-parser-xml": "2.0.1",
    "chalk": "4.1.0",
    "cors": "2.8.5",
    "ejs": "3.1.5",
    "express": "4.17.1",
    "express-rate-limit": "5.2.3",
    "fs-extra": "9.0.1",
    "http-proxy-middleware": "^1.0.6",
    "inquirer": "7.3.3",
    "lodash.merge": "^4.6.2",
    "loglevel": "1.7.0",
    "loglevel-plugin-prefix": "0.8.4",
    "prettier": "2.1.2",
    "slash": "3.0.0",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "4.0.5",
    "url-pattern": "1.0.3",
    "yargs": "16.1.0"
  },
  "devDependencies": {
    "@types/body-parser-xml": "1.1.0",
    "@types/cors": "2.8.9",
    "@types/eslint": "7.2.6",
    "@types/fs-extra": "9.0.6",
